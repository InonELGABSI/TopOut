CREATE TABLE track_points (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    sessionId TEXT NOT NULL,
    ts INTEGER NOT NULL,
    lat REAL,
    lon REAL,
    altitude REAL,
    accelX REAL,
    accelY REAL,
    accelZ REAL,
    gain REAL,
    loss REAL,
    relAltitude REAL,
    avgHorizontal REAL,
    avgVertical REAL,
    danger INTEGER,
    alertType TEXT
);

insertTrackPoint:
INSERT INTO track_points(sessionId, ts, lat, lon, altitude, accelX, accelY, accelZ,
                         gain, loss, relAltitude, avgVertical, avgHorizontal,
                         danger, alertType)
VALUES (?, ?, ?,  ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?);

getTrackPointsBySession:
SELECT * FROM track_points WHERE sessionId = ?;

deleteTrackPointsBySession:
DELETE FROM track_points WHERE sessionId = ?;

lastInsertRowId:
SELECT last_insert_rowid() AS id;

